<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <!-- Bootstrap CSS -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

        <title></title>
        <style>
            .heading{
                font-size: 25px;
    
                font-weight: 600;
            }
            body{
                background: url(http://wrbbradio.org/wp-content/uploads/2016/10/grey-background-07.jpg) ;
                
            }
           </style>
    </head>
    <body>
        <div class="card m-5 shadow-lg" style="background-color:#e8e9eb; ">
            <div class="card-header text-center heading  ">Health Center Timings</div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered">
                    <tr>
                        
                       
                        <th>Doctor Name</th>
                        <th>Qualification</th>
                        
                        <th>SPECIALIZATION</th> 
                        <th>Timings</th> 
                        <th>Avialability</th> 
                        
                       
    
                    </tr>
                    <%
                    if(sampleData.length > 0)
                    {
                        sampleData.forEach(function(data){
                    %>
                    <tr>
                       
                        
                        <td><%= data.name%></td>
                        <td><%= data.qualification%></td>
                        <td><%= data.specialization %></td>
                        <td><%= data.timings %></td>
                        <td><%= data.available %></td>
                       
                       
                    </tr>
                    <%
                        });
                    }
                    else
                    {
                    %>
                    <tr>
                        <td colspan="2">No Data Found</td>
                    </tr>
                    <%
                    }
                    %>
                </table>
            </div>
        </div>
        <h6 style="color:rgb(195, 77, 77) ;">NOTE: </h6>
        <div style="color:rgb(195, 77, 77); margin-left: 60px; margin-top: -30px; text-size-adjust:inherit  ;">
       <%
                    if(sampleData1.length > 0)
                    {
                        sampleData1.forEach(function(data1){
                    %>
                    <tr >
                       
                        
                        <td ><%= data1.note%></td>
                      
                       
                    </tr>
                    <%
                        });
                    }
                    else
                    {
                    %>
                    <tr> 
                      
                        <td colspan="2">No Data Found</td>
                    </tr>
                    <%
                    }
                    %>
                </div>
                <br>
</div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    </body>
</html>