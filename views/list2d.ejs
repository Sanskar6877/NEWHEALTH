<!doctype html>
<html lang="en">
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <!-- Bootstrap CSS -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

        <title></title>
        <style>
            .heading{
                font-size: 25px;
    
                font-weight: 600;
            }
            body{
                background-color:  rgb(196, 220, 242) ;                       
                
            }
           </style>
    </head>
    <body>
        <form >
            <button  type="submit" formaction="/login1/doctordashboards/<%= data5%>" style=" background-color:rgb(66, 154, 242); color:rgb(49, 48, 48);text-decoration:none; margin-left: 1550px; width:60px ">Back</button>
        </form>
        <div class="card m-5 shadow-lg" style="background-color: rgb(131, 188, 245); ">
            <div class="card-header text-center heading  ">Health Center Timings</div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table" style="color: black; border-color: black;">
                    <tr>
                        
                       
                        <th>Doctor Name</th>
                        <th>Qualification</th>
                        
                        <th>SPECIALIZATION</th> 
                        <th>Timings</th> 
                        <th>Avialability</th> 
                        
                       
    
                    </tr>
                    <%
                    if(sampleData.length > 0)
                    {
                        sampleData.forEach(function(data){
                    %>
                    <tr>
                       
                        
                        <td><%= data.name%></td>
                        <td><%= data.qualification%></td>
                        <td><%= data.specialization %></td>
                        <td><%= data.timings %></td>
                        <td><%= data.available %></td>
                       
                       
                    </tr>
                    <%
                        });
                    }
                    else
                    {
                    %>
                    <tr>
                        <td colspan="2">No Data Found</td>
                    </tr>
                    <%
                    }
                    %>
                </table>
            </div>
        </div>
        <br>
        <h5 style="color:rgb(71, 17, 231) ;">Note : </h5>
        <div style="color:rgb(71, 17, 231); margin-left: 70px; margin-top: -30px; text-size-adjust: inherit;">
     <%
                    if(sampleData1.length > 0)
                    {
                        sampleData1.forEach(function(data1){
                    %>
                    <tr >
                       
                        
                        <td ><%= data1.note%></td>
                      
                       
                    </tr>
                    <%
                        });
                    }
                    else
                    {
                    %>
                    <tr> 
                      
                        <td colspan="2">No Data Found</td>
                    </tr>
                    <%
                    }
                    %>
                </div>
                <br>
</div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    </body>
</html>